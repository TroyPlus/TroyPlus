        $("#totalPurchase1").val(tot.toFixed(2));

    }

    function CalculateDiscount() {
        var tot = 0;

        for (var j = i; j >= 0; j--) {
            if (!isNaN(parseFloat($("#goodreceiptitemlist_" + j + "__Discount_percent").val()))) {
                tot = parseFloat(tot) + parseFloat($("#goodreceiptitemlist_" + j + "__Discount_percent").val());
            }
        }

        $("#totalDiscount1").val(tot.toFixed(2));

