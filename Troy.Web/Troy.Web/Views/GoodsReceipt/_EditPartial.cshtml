    function OnChangeProduct(val, rowId) {

        //debugger;
        var pID = parseInt(val.value);
        var dataUrl = "../GoodsReceipt/GetPrice?pID=" + pID;

        $.ajax({
            type: 'GET',
            url: dataUrl,
            data: pID,
            async: true,
            cache: false,
            crossDomain: true,
            contentType: "application/json; charset=utf-8",
            dataType: 'json',
            error: function (xhr, err) {
            },
            success: function (result, status) {
                //debugger;
                var id = "#goodreceiptitemlist_" + rowId + "__Unit_price";
                $(id).val(result);

                //onchange quantity functionality
                var qty = $("#goodreceiptitemlist_" + rowId + "__Quantity").val();
                var price = $("#goodreceiptitemlist_" + rowId + "__Unit_price").val();
                var totAmount = price * qty;
                var id = "#goodreceiptitemlist_" + rowId + "__LineTotal";
                $(id).val(totAmount.toFixed(2));

                //onchange discount functionality
                var dis = $("#goodreceiptitemlist_" + rowId + "__Discount_percent").val();
                var qty = $("#goodreceiptitemlist_" + rowId + "__Quantity").val();
                var price = $("#goodreceiptitemlist_" + rowId + "__Unit_price").val();
                var totAmount = price * qty - ((price * qty) * dis / 100)
                var id = "#goodreceiptitemlist_" + rowId + "__LineTotal";
                $(id).val(totAmount.toFixed(2));

                CalculateTotal();
                CalculateTotalAmount();
                CalculateTax();
            }
        });
    }

